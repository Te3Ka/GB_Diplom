<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".schedule_meeting.ScheduleMeetingFragment">

    <Button
        android:id="@+id/button_invitationMeeting"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_invitation_a_meeting"
        app:layout_constraintBottom_toTopOf="@id/button_viewMeetings"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/button_viewMeetings"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/button_view_meetings"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/button_invitationMeeting" />


</androidx.constraintlayout.widget.ConstraintLayout>